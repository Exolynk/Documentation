  "layout": "<record>\n    <!-- Lead Informationen -->\n    <tab name=\"Lead Informationen\" active=\"true\" status=\"\">\n        <data name=\"Pipeline\">\n            <variable ident=\"status\" access=\"write\" />\n            <variable ident=\"crm_prio_select\" access=\"write\" />\n            <variable ident=\"opportunity_value_revenue\" access=\"write\" />\n            <variable ident=\"opportunity_value_licenses\" access=\"write\" />\n            <variable ident=\"exl_product_select\" access=\"write\" />\n            <variable ident=\"forecast_completion\" access=\"write\" />\n            <variable ident=\"assigned_user_1\" access=\"write\" />\n        </data>\n        <data name=\"Company Information\">\n            <variable ident=\"industry\" access=\"write\" />\n            <variable ident=\"employees\" access=\"write\" />\n            <variable ident=\"client_start_date\" access=\"write\" />\n        </data>\n        <data name=\"Company Contact Details\">\n            <variable ident=\"ident\" name=\"Kundennummer\" access=\"write\" />\n            <variable ident=\"name\" access=\"write\" />\n            <variable ident=\"description\" access=\"write\" />\n            <variable ident=\"client_info\" access=\"write\" />\n            <variable ident=\"client_phone\" access=\"write\" />\n            <variable ident=\"client_email\" access=\"write\" />\n            <variable ident=\"client_website\" access=\"write\" />\n            <variable ident=\"client_address\" access=\"write\" />\n        </data>\n        <data name=\"Contact Person\">\n            <variable ident=\"contact_title\" access=\"write\" />\n            <variable ident=\"contact_firstname\" access=\"write\" />\n            <variable ident=\"contact_lastname\" access=\"write\" />\n            <variable ident=\"contact_phone\" access=\"write\" />\n            <variable ident=\"contact_mobile\" access=\"write\" />\n            <variable ident=\"contact_email\" access=\"write\" />\n            <variable ident=\"contact_linkedin\" access=\"write\" />\n        </data>\n    </tab>\n    \n    <!-- CRM Activities -->\n    <tab name=\"Activities\" active=\"true\" status=\"\">\n        <data name=\"CRM Activities\">\n            <variable ident=\"crm_activities\" access=\"write\" />\n        </data>\n    </tab>\n    \n    <!-- CRM Taskes -->\n    <tab name=\"Taskes\" active=\"true\" status=\"\">\n        <data name=\"CRM Taskes\">\n            <variable ident=\"crm_taskes\" access=\"write\" />\n        </data>\n    </tab>\n    \n    <!-- Attachements -->\n    <tab name=\"Attachements\" active=\"true\" status=\"\">\n        <data name=\"Files\">\n            <variable ident=\"files\" access=\"write\" />\n        </data>\n        <files variable=\"files\" />\n    </tab>\n    \n    <!-- Chart Opportunity by Stage -->\n    <tab name=\"Chart\">\n        <chart typ=\"bar\" name=\"Opportunity by Stage\">\n            <chart-data model=\"opportunities\" action=\"count\" variable=\"status\" group_by=\"ident\" name=\"New\">\n                <data-filter variable=\"status\" operator=\"equal\" value=\"new\" />\n            </chart-data>\n            <chart-data model=\"opportunities\" action=\"count\" variable=\"status\" group_by=\"ident\" name=\"1/7 Prospecting\">\n                <data-filter variable=\"status\" operator=\"equal\" value=\"prospecting\" />\n            </chart-data>\n            <chart-data model=\"opportunities\" action=\"count\" variable=\"status\" group_by=\"ident\" name=\"2/7 Qualification\">\n                <data-filter variable=\"status\" operator=\"equal\" value=\"qualification\" />\n            </chart-data>\n            <chart-data model=\"opportunities\" action=\"count\" variable=\"status\" group_by=\"ident\" name=\"3/7 Needs Analysis\">\n                <data-filter variable=\"status\" operator=\"equal\" value=\"needs_analysis\" />\n            </chart-data>\n            <chart-data model=\"opportunities\" action=\"count\" variable=\"status\" group_by=\"ident\" name=\"4/7 Proposal\">\n                <data-filter variable=\"status\" operator=\"equal\" value=\"proposal\" />\n            </chart-data>\n            <chart-data model=\"opportunities\" action=\"count\" variable=\"status\" group_by=\"ident\" name=\"5/7 Negotiation\">\n                <data-filter variable=\"status\" operator=\"equal\" value=\"negotiation\" />\n            </chart-data>\n            <chart-data model=\"opportunities\" action=\"count\" variable=\"status\" group_by=\"ident\" name=\"6/7 Rollout\">\n                <data-filter variable=\"status\" operator=\"equal\" value=\"won_closed\" />\n            </chart-data>\n            <chart-data model=\"opportunities\" action=\"count\" variable=\"status\" group_by=\"ident\" name=\"7/7 Closed Won\">\n                <data-filter variable=\"status\" operator=\"equal\" value=\"won_closed\" />\n            </chart-data>\n            <chart-data model=\"opportunities\" action=\"count\" variable=\"status\" group_by=\"ident\" name=\"7/7 Closed Lost\">\n                <data-filter variable=\"status\" operator=\"equal\" value=\"lost_closed\" />\n            </chart-data>\n        </chart>\n    </tab>\n\n    <!-- Logs -->    \n    <tab name=\"Logs\">\n        <record-logs />\n    </tab>\n</record>",
